                            button(type='button' data-filter='.tests').btns tests

                            button(type='button' data-filter='.editorials').btns editorials

                            button(type='button' data-filter='.snaps').btns snaps
                        .section_tabs__body
                            each elem in imgArr
                                a(href=elem.src class=elem.data data-myorder=elem.order).mix
                                    img(src=elem.src )
                    .section.section_forms
                        .section__title
                            span contact Model
                        .section_forms__body
                            form(novalidate)
                                .forms-group
                                    .span.icon.user-outline-gray
                                    input(type='text' name='userName' class='forms-control' required)
                                .forms-group
                                    .span.icon.contact-gray
                                    input(type='email' name='userEmail' class='forms-control' required)
                                .forms-group
                                    .span.icon.smartphone-gray
                                    input(type='tel' name='userTel' class='forms-control' required)
                                .forms-group
                                    .span.icon.edit-grey
                                    textarea(class='forms-control' required name='userMessage' )
                                .forms-group
                                    button(type='submit').btns.primary send
                                    button(type='reset').btns clear

                .col-md-8.col-lg-5
                    .section_model
                        .section_model__photo
                            img(src='assets/img/temp/model.jpg')
                        .section_model__name
                            span Whitney Seether
                        .section_model__soc.soc-btn
                            ul
                                li.fb
                                    a(href='javascript:void(0)').icon.facebook
                                li.tw
                                    a(href='javascript:void(0)').icon.twitter
                                li.g
